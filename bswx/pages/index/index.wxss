
.swiper-tab {
  width: 100%;
  height: 76rpx;
  box-shadow: 0 -2px 0 #ccc inset;  
  background-color: #fff;
}
.getLand image{
  width: 22rpx;
  height: 29rpx;
  margin-left: 30rpx;
}
#btn{
  width:662rpx;
  height: 160rpx;
  line-height: 160rpx;
  margin-left: 44rpx;
  margin-top:40rpx;
  margin-bottom: 50rpx;
  background-image: linear-gradient(-48deg,#FECC00  0%, #FFEB00 100%);
  box-shadow: 0 2px 6px 0 rgba(0,0,0,0.10);
  border-radius: 100px;
  font-family: PingFangSC-Regular;
  font-size: 50rpx;
  color: #000000;
  letter-spacing: 0;
  border: 0;
}
.swiper-tab-list {
  font-size: 36rpx;
  display: inline-block;
  width: 160rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #696363;
  text-align: center;  
  margin: 0 32rpx;

}
.on {
  color: #090909;
  border-bottom:6rpx solid #FFC100;  
}


.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}


.swiper-box view {
  text-align: center;
}

.contentel_l{
  float: left;
  border-right: 1px solid #9b9b9b; 
}
.contentel_r{
  margin-left: 225rpx;
}
.contentel_top{
  margin: 42rpx auto;
  width: 186rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  font-weight: 500;
  font-size:30rpx;
  border-radius: 200rpx;
  color: #5D5D5D;
  background: white;
  letter-spacing: 0;
}
.contentel_top .conten_icon{
    width: 32rpx;
    height: 32rpx;
    float: left;
    margin-left: 10rpx;
}
.contentel_top .conten_icon image{
  width: 100%;
  height: 100%;
}
.contentel_top.oncont_seclt{
  background: rgba(255,194,0,0.08);
} 
.contentel_top_r{
  border-bottom: 1px solid #ccc;
  padding:24rpx 30rpx 24rpx 0rpx;
}
.conten_shopfix{
  position: fixed;
  right: 10rpx;
  bottom: 130rpx;
}
.conten_shopfix text{
  min-width: 28rpx;
  padding: 0 6rpx;
  height: 28rpx;
  font-size: 20rpx;
  background: #FB3D3D;
  border-radius: 100%;
  color: white;
  position: absolute;
  right: 0;
  top: 20rpx;
}
.conten_shopfix image{
  width: 104rpx;
  height: 104rpx;
}
.contentel_top_r .contentel_top_rl{
  width: 152rpx;
  height: 152rpx;
  float: left;
}
.contentel_top_r .contentel_top_rr{
  margin-left: 172rpx;
}
.contentel_top_rr .conten_top_rr_1{
    font-size: 36rpx;
    text-align: left;
}
.contentel_top_rr .conten_top_rr_2{
    font-size: 24rpx;
    text-align: left;  
}
.contentel_top_rr .conten_top_rr_2 image{
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
}
.contentel_top_rr .conten_top_rr_2 text{  
  margin: 1rpx 0 0 10rpx;
}
.contentel_top_rr .conten_top_rr_3{
    font-size: 44rpx;
    text-align: left;
    margin-top: 30rpx;
    color: #FB3D3D ;
}
.conten_top_3r image{
  width: 44rpx;
  height: 44rpx;
}
.conten_top_3r{
  float: right;
  font-size: 30rpx;
  margin-top: 8rpx;
}
.conten_top_3r.zeroHidden .conten_jian,
.conten_top_3r.zeroHidden text
{
  display: none;
}
.conten_top_3r text{
  float: left;
  margin: 0 10rpx;
}
.conten_jian,
.conten_jia{
  width: 55rpx;
  height: 55rpx;
  float: left;
}
.contentel_top_r .contentel_top_rl image{
  width: 100%;
  height: 100%;
}
.shicai_r{
  width: 100%;
}
